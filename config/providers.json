{
  "providers": {
    "anthropic": {
      "enabled": true,
      "requires_key_env": "ANTHROPIC_API_KEY",
      "models": [
        "claude-opus-4.6",
        "claude-sonnet-4.6",
        "claude-haiku-4.5"
      ]
    },
    "ollama": {
      "enabled": true,
      "requires_key_env": null,
      "base_url_env": "LOCAL_OPENAI_BASE_URL",
      "models": [
        "llama-4-8b-instruct",
        "mistral-small",
        "qwen2.5:7b-instruct"
      ]
    },
    "zhipu": {
      "enabled": false,
      "requires_key_env": "ZHIPU_API_KEY",
      "models": [
        "glm-4.6",
        "glm-4.5-air"
      ]
    }
  },
  "roles": {
    "ace": {
      "primary": {
        "provider": "anthropic",
        "model": "claude-opus-4.6"
      },
      "fallbacks": [
        {
          "provider": "anthropic",
          "model": "claude-sonnet-4.6"
        },
        {
          "provider": "anthropic",
          "model": "claude-haiku-4.5"
        },
        {
          "provider": "ollama",
          "model": "llama-4-8b-instruct"
        }
      ]
    },
    "worker": {
      "primary": {
        "provider": "ollama",
        "model": "llama-4-8b-instruct"
      },
      "fallbacks": [
        {
          "provider": "ollama",
          "model": "mistral-small"
        },
        {
          "provider": "anthropic",
          "model": "claude-haiku-4.5"
        }
      ]
    }
  },
  "defaults": {
    "role": "ace"
  },
  "open_fall": {
    "strategy": "degrade_log_notify",
    "discord_webhook_env": "DISCORD_ALERT_WEBHOOK",
    "queue_non_critical_tasks": true,
    "local_reflex_model": {
      "provider": "ollama",
      "model": "llama-4-8b-instruct"
    }
  }
}
